#!/bin/sh

# loads every other file in this same directory
currentDir="$(dirname "${BASH_SOURCE}")";

for file in $currentDir/{aliases,editor,exports,history,workspace}.sh; do
	[ -r "$file" ] && [ -f "$file" ] && source "$file";
done;

unset file;
unset currentDir;

export JAVA_TOOL_OPTIONS="-Dfile.encoding=UTF8 -Duser.country=US -Duser.language=en"
export JAVA_OPTIONS="-Dfile.encoding=UTF8 -Duser.country=US -Duser.language=en"

